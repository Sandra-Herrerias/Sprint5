{
  "compilerOptions": {
    "target": "es5",
    "lib": [ "es6", "dom"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictBindCallApply": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true, // HERE
    "module": "es6",
    "moduleResolution": "node",
    "isolatedModules": true,
    "noEmit": true,
    "sourceMap": true
  }
}